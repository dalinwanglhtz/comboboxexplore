<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.collections.*?>
<?import java.lang.*?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="application.MainController">
   <children>
      <ComboBox fx:id="combobox" layoutX="58.0" layoutY="129.0" onAction="#comboChanged" prefHeight="43.0" prefWidth="185.0" promptText="Select Name" >
      	<items>
      		<FXCollections fx:factory="observableArrayList">
      			<String fx:value="This is insane"/>
      			<String fx:value="This is more insane"/>
      			<String fx:value="This is extremely insane"/>
      		</FXCollections>
      	</items>
      </ComboBox>
      <Label fx:id="myLabel" layoutX="58.0" layoutY="77.0" prefHeight="32.0" prefWidth="185.0" text="Names" />
      <Button layoutX="117.0" layoutY="204.0" mnemonicParsing="false" onAction="#buttonAction" text="Button" />
   </children>
</AnchorPane>
